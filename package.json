{
  "name": "markdown-image-upload",
  "displayName": "markdown-image-upload",
  "description": "upload images in markdown to github",
  "publisher": "Maidang",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.66.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Maidang1/markdown-image-upload"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "upload-image",
        "title": "Upload Image",
        "category": "markdown"
      },
      {
        "command": "markdown-setting",
        "title": "Markdown Setting",
        "category": "markdown"
      },
      {
        "command": "markdown-image-preview",
        "title": "Markdown Image Preview",
        "category": "markdown"
      },
      {
        "command": "markdown-image-paste",
        "title": "Markdown Image Paste",
        "category": "markdown"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "upload-image",
          "group": "group1",
          "when": "editorHasSelection"
        },
        {
          "command": "markdown-image-paste",
          "group": "group1",
          "when": "editorTextFocus && editorLangId == 'markdown'"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "mocha": "^9.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "glob": "^8.0.1",
    "octokit": "^1.7.1"
  }
}
